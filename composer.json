{
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "name": "ryunosuke/ltsview",
  "type": "command",
  "description": "pretty log view",
  "license": "MIT",
  "authors": [
    {
      "name": "ryunosuke",
      "email": "ryunosuke.arima@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "symfony/console": "4.*",
    "symfony/yaml": "*",
    "phpseclib/phpseclib": "3.*"
  },
  "require-dev": {
    "ryunosuke/functions": "1.6.*",
    "phpunit/phpunit": "9.*"
  },
  "autoload": {
    "psr-4": {
      "ryunosuke\\ltsv\\": "src"
    },
    "files": [
      "src/functions.stub"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ryunosuke\\test\\": "tests/Test"
    },
    "files": [
      "tests/functions.php"
    ]
  },
  "bin": [
    "bin/logrep"
  ],
  "scripts": {
    "build": [
      "@composer install",
      "@composer export",
      "@composer test",
      "@composer box"
    ],
    "export": [
      "export-function",
      "exclude-internal"
    ],
    "test": [
      "vendor/bin/phpunit --configuration=tests/phpunit.xml"
    ],
    "box": [
      "@composer dump-autoload --no-dev",
      "bin/box.phar build -c box.json",
      "@composer dump-autoload"
    ],
    "post-install-cmd": [
      "@export"
    ],
    "post-update-cmd": [
      "@export"
    ]
  }
}
